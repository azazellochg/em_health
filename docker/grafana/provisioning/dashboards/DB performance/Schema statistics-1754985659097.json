          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  a.attname AS \"Name\",\n  pg_catalog.format_type(a.atttypid, a.atttypmod) AS \"Type\",\n  ROUND((s.null_frac * 100)::numeric, 2) AS \"Null %\",\n  s.avg_width AS \"Avg. size (bytes)\",\n  CASE\n    WHEN s.n_distinct < 0 THEN\n      (ROUND(((-s.n_distinct) * c.reltuples)::numeric)::TEXT || '*')\n    ELSE\n      s.n_distinct::TEXT\n  END AS \"Num. distinct\",\n  CASE\n    WHEN EXISTS (\n      SELECT 1\n      FROM pg_index i\n      WHERE\n        i.indrelid = a.attrelid\n        AND a.attnum = ANY(i.indkey)\n    ) THEN 'yes'\n    ELSE 'no'\n  END AS \"Indexed?\"\nFROM\n  pg_attribute a\n  JOIN pg_class c ON a.attrelid = c.oid\n  JOIN pg_namespace n ON c.relnamespace = n.oid\n  LEFT JOIN pg_stats s ON\n    s.schemaname = n.nspname\n    AND s.tablename = c.relname\n    AND s.attname = a.attname\nWHERE\n  s.schemaname = '$schema'\n  AND s.tablename = '$table'\n  AND a.attnum > 0\n  AND NOT a.attisdropped\nORDER BY\n  a.attnum;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
